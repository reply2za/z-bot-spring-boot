<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="helpHandler" class="hoursofza.commands.Help" />
    <bean id="pingHandler" class="hoursofza.commands.Ping" />

    <bean id="messageEventListener" class="hoursofza.listeners.MessageReceivedListener">
        <constructor-arg>
            <set>
                <ref bean="helpHandler"/>
                <ref bean="pingHandler"/>
            </set>
        </constructor-arg>
    </bean>
</beans>